<script type="text/javascript">
    //<![CDATA[
    var SW_MENU_POPUP_WIDTH = 0;
    jQuery(function($){
        $('.menu-icon, .close-mobile-nav, .close-sidebar-menu, .header-container.type10 .dropdown-menu > .menu-container > a, .header-container.type28 .dropdown-menu > .menu-container > a').off("mousedown").on("mousedown",function(){
            $this = $(this);
            if ($(".menu_holder_desktop.loading").length && ($(window).width()>991)) {
                $.ajax({
                    url: "https://dijaspora.shop/staticmenu/index/showmenu",
                    dataType: 'json',
                    type: 'post',
                    success: function(data) {
                        $(".menu_holder_desktop.loading").removeClass("loading");
                        $(".menu_holder_desktop").html(data['popup_content']).hide().fadeIn();
                        $(".menu_holder_desktop").find(">li:first-child").addClass("hovered");
                        $('.menu_holder_desktop').et_menu({
                            type: "default"
                        });
                    },
                    error: function(){
                        $(".menu_holder_desktop.loading").removeClass("loading");
                    }
                });
            } else if ($(".menu_holder_mobile.loading").length && ($(window).width()<=991)) {
                // $(".menu-icon, .header-container.type10 .dropdown-menu > .menu-container > a").off("mousedown").on("mousedown",function(){
                    // if (!$(".mobile-nav ul.menu").hasClass("ajax-loaded") && !$(".mobile-nav ul.menu").hasClass("ajax-loading")) {
                        $(".mobile-nav ul.menu").addClass("ajax-loading");
                        $.ajax({
                            url: "https://dijaspora.shop/swmegamenu/index/showmobilemenu",
                            dataType: 'json',
                            type: 'post',
                            success: function(data) {
                                $(".menu_holder_mobile.loading").removeClass("loading");
                                $(".mobile-nav ul.menu").removeClass("ajax-loading").addClass("ajax-loaded").html(data['popup_content']).hide().fadeIn();
                                $(".mobile-nav ul.menu").find("li.menu-parent-item").prepend('<span class="open-child">(open)</span>');
                                $(".mobile-nav ul.menu").find(".open-child").off("click").on("click", function(){
                                    if(!$(this).parent().hasClass("over")) {
                                        $(this).parent().parent().find(".over > ul").slideUp();
                                        $(this).parent().parent().find(".over").removeClass("over");
                                        $(this).parent().addClass("over");
                                        $(this).parent().find(">ul").slideDown();
                                    } else {
                                        $(this).parent().removeClass("over");
                                        $(this).parent().find(">ul").slideUp();
                                    }
                                });
                            }
                        });
                    // }
                // });
            }
        });
    });
    //]]>
</script>
