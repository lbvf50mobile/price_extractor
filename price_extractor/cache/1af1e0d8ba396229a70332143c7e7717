jQuery(function($){
	var scrolled = false;
	$(window).scroll(function(){
	    if(1000<$(window).scrollTop() && !scrolled){
	    	if ($(".menu_holder_desktop").hasClass("show")) {
	    		$(".dropdown-menu .menu-container > a").click();
	    	}
            if(!$('.header-container .menu-wrapper .mini-cart').length && !$('.header-container .menu-wrapper .sticky-logo').length){
			    $('.header-container').addClass("sticky-header");
			    var minicart = $('.header-container .mini-cart').html();
			    $('.header-container .menu-wrapper').append('<div class="mini-cart">'+minicart+'</div>');
			    var currency = $('.dijasporashop_header_heading').find('.currency_and_date_header');
			    $('.header-container .menu-wrapper').prepend('<div class="f-left currency_and_date_header">'+currency.html()+'</div>');
			    $('.header-container .menu-wrapper').find('.currency_and_date_header select').val(currency.find('select').val());
		    	currency.remove();
	            			    $('.header-container .header-wrapper > div').each(function(){
				    if($(this).hasClass("container")){
					    $(this).addClass("already");
				    } else {
					    $(this).addClass("container");
				    }
			    });
			    scrolled = true;
            }
	    }
	    if(1000>=$(window).scrollTop() && scrolled){
		    $('.header-container').removeClass("sticky-header");
		    $('.header-container .menu-wrapper .mini-cart').remove();
		    var currencySticky = $('.header-container .menu-wrapper').find('.currency_and_date_header');
		    $('.dijasporashop_header_heading .header_icons').prepend('<div class="f-left currency_and_date_header">'+currencySticky.html()+'</div>');
			$('.dijasporashop_header_heading .header_icons').find('.currency_and_date_header select').val(currencySticky.find('select').val());
	    	currencySticky.remove();
            		    scrolled = false;
		    $('.header-container .header-wrapper > div').each(function(){
			    if($(this).hasClass("already")){
				    $(this).removeClass("already");
			    } else {
				    $(this).removeClass("container");
			    }
		    });
	    }
	});
});
